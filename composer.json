{
    "name": "jeremynikolic/laravel-revision",
    "description": "A package to observer model, and their relations, changes and track them in database",
    "keywords": [
        "jeremynikolic",
        "laravel-revision"
    ],
    "homepage": "https://github.com/jeremynikolic/laravel-revision",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jeremy Nikolic",
            "email": "nikolic.jeremy@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/support": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "JeremyNikolic\\Revision\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JeremyNikolic\\Revision\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "RevisionServiceProvider"
            ]
        }
    }
}
